{
  "name": "productapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "jest --detectOpenHandles --watchAll=false --force-exit",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "pretest": " npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "lint": "eslint . "
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "^4.16.4",
    "express-rate-limit": "^5.3.0",
    "express-validator": "^6.12.1",
    "googleapis": "^84.0.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-errors": "~1.6.3",
    "i18next": "^20.4.0",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-middleware": "^3.1.4",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.0",
    "node-cron": "^2.0.3",
    "sequelize-cli": "^6.2.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "xss-clean": "^0.1.1"
  },
  "main": "app.js",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "faker": "^5.5.3",
    "install": "^0.13.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "pg": "^8.7.1",
    "prettier": "^2.3.2",
    "sequelize": "^6.6.5",
    "snpm": "^0.1.1",
    "supertest": "^6.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "eqeqeq": "warn",
      "prettier/prettier": "warn",
      "no-unused-vars": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printwidth": 120
  }
}
